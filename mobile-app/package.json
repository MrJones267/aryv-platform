{
  "name": "aryv-mobile",
  "version": "1.0.0",
  "description": "ARYV mobile application built with React Native",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "react-native start",
    "test": "jest",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "type-check": "tsc --project tsconfig.production.json",
    "type-check:strict": "tsc --noEmit",
    "clean": "react-native clean",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "build:ios": "cd ios && xcodebuild -workspace ARYV.xcworkspace -scheme ARYV -configuration Release archive",
    "build:ios:release": "react-native run-ios --configuration Release",
    "build:production": "npm run type-check && npm run lint && npm run test && npm run build:android && npm run build:ios",
    "bundle:analyze": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res --sourcemap-output android/sourcemap.js",
    "icons:generate": "node scripts/generate-app-icons.js",
    "icons:clean": "rm -rf android/app/src/main/res/mipmap-* ios/aryv/Images.xcassets/AppIcon.appiconset/*.png",
    "preinstall": "npx check-engine",
    "postinstall": "echo 'Skipping iOS pod install for Android-only build'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^18.6.1",
    "@react-native-firebase/messaging": "^18.6.1",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "6.9.26",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.12.0",
    "crypto-js": "^4.2.0",
    "deprecated-react-native-prop-types": "^5.0.0",
    "react": "18.2.0",
    "react-native": "^0.72.7",
    "react-native-chart-kit": "^6.12.0",
    "react-native-fs": "^2.20.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "2.14.1",
    "react-native-image-picker": "^5.6.0",
    "react-native-maps": "1.10.0",
    "react-native-paper": "^5.11.3",
    "react-native-permissions": "^3.10.1",
    "react-native-reanimated": "^3.6.3",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^13.4.0",
    "react-native-vector-icons": "^10.0.2",
    "react-native-vision-camera": "^3.9.0",
    "react-native-webrtc": "^124.0.7",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^11.3.10",
    "@react-native-community/cli-platform-android": "^11.3.10",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "@types/webrtc": "^0.0.47",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-jest": "^29.2.1",
    "detox": "^20.13.5",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-native-document-picker": "^9.3.1",
    "react-native-performance": "^5.1.4",
    "react-test-renderer": "18.2.0",
    "sharp": "^0.34.5",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}
